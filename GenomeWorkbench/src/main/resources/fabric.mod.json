{
  "schemaVersion": 1,
  "id": "morerealisticgeneediting",
  "version": "${version}",
  "name": "More Realistic Gene Editing",
  "description": "A mod that adds more realistic gene editing features to Minecraft, simulating real-world genetic engineering techniques.",
  "authors": [
    "MRGE Development Team"
  ],
  "contact": {
    "homepage": "https://github.com/craftbukkit/More-realistic-gene-editing",
    "sources": "https://github.com/craftbukkit/More-realistic-gene-editing"
  },
  "license": "MIT",
  "icon": "assets/morerealisticgeneediting/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.morerealisticgeneediting.MoreRealisticGeneEditing"
    ],
    "client": [
      "com.morerealisticgeneediting.MoreRealisticGeneEditingClient"
    ]
  },
  "mixins": [
    "genomeworkbench.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.0",
    "fabric-api": "*",
    "minecraft": "~1.21",
    "java": ">=21"
